# import <รง>
# import <ft>

macro struct
{
	int	comment				:1;
	int	comments			:1;
	int	quote				:1;
	int	quotes				:1;
	int	escaped				:1;
	int	parentheses_level	:16;
	int	brackets_level		:16;
	int	braces_level		:16;
}		c_ctx;

macro rule	test(const char *s)
{
	return (rule) {0, 0};
}

macro rule	lol(const char *s, size_t i)
{
	char	name[255];
	char	*o;
	size_t	i;

	i = 0;
	while (s[i] && s[i] == "Hello"[i])
		i += 1;
	while (s[i] && s[i] != '.')
	{
		name[i] = s[i];
		i += 1;
	}
	if (!s[i])
		return (rule) {0, 0};
	name[i] = 0;
	asprintf(&o, "Hello %s!", name);
	return (rule) {i, o};
}

int	main(void)
{
	printf("Hello joe.");
}
